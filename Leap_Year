#include<stdio.h>
#include<conio.h>
void main()
{
	int year;
	clrscr();
	printf("\n Enter year:");
	scanf("%d",&year);
	if(year%4==0 && year%100==0)
	{
		if(year%400==0)
		{
			printf("\n %d is Leap Year ");
		}

	}
	else if(year%4==0)
	{
		printf("\n %d is Leap Year ");
	}
	else
	{
		printf("\n %d is not Leap Year");
	}
getch();
}
